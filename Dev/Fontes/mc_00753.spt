	---------------------------------------------------------------------------------------------------------------------
   Legenda: [-] - Correção / [*] - Recurso modificado/melhorado / [+] - Implementação de recurso
*/
Create or Replace Function mc_00753####???()
Returns trigger
As
$$
Declare

BEGIN
   If tg_op <> 'DELETE' Then
      if new.nf3_data_desemb > (select nf1_dt_emissao
                                  from [(nf1)]
                                 where nf1_serial = new.nf1_serial) then
         raise '[[A data do desembaraço não pode ser superior a data da emissão da Nota Fiscal. verifique!]]';
      end if;

      Return New;
   Else
      Return Old;
   End If;
End;
$$ language plpgsql;
