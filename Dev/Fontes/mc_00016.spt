Create or Replace Function mc_00016####???
() Returns trigger 
As $$
Begin
   if tg_op = 'UDATE' then
      raise notice '[[Registros de aferição de custo não podem ser alterados]]';
   end if;

   if tg_op = 'INSERT' then
      new.sch_data:= current_timestamp;
      return new;
   else
      return old;
   end if;
End;
$$ language plpgsql;