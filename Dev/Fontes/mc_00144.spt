Create or Replace Function mc_00144####???() 
  Returns trigger AS $$
Declare

begin
   If tg_op = 'DELETE' then
      if (mc_getflag####???('SA5',old.sa5_codorc) = 0) and
         (old.ga0_altera = 0) then
         raise '[[ATENÇÃO. Não é possivel excluir este registro.]]';
      end if;
      Return Old;
   End If;
   Return New;
End;
$$  LANGUAGE plpgsql;