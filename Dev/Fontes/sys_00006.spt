/**
   Retorna espaços referente a identação

   Autor	      Ricardo Gonçalves
   Data        07/08/2014 17:05:00
   Trigger     
   
   @param in in_nivel Nível de identação desejado   

	Histórico
	--------------------------------------------------------------------------------------------------------------------	
*/
create or replace function sys_00006####???(
   in in_nivel integer)
returns varchar
as
$$
begin
   in_nivel := coalesce(in_nivel,0);
   if in_nivel = 0 then
      return '';
   else   
      return repeat('  ', in_nivel);
   end if;
end;
$$
language plpgsql;