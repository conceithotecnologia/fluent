          cTexto := cTexto || sTipo;                                           --018/032-Tipo/Numero de Inscricao
          cTexto := cTexto || rpad(xBanco.convenio,20);                        --033/052-Convenio
          cTexto := cTexto || rpad(xBanco.a0_codag::varchar,05);               --053/057-Agência Mantenedora da Conta
          cTexto := cTexto || rpad(xBanco.a0_digito,        01);               --058/058-Dígito Verificador da Agência
          cTexto := cTexto || rpad(xBanco.ak_conta,         12);               --059/070-Número da Conta Corrente
          cTexto := cTexto || rpad(xBanco.ak_digito,        01);               --071/071-Dígito Verificador da Conta
