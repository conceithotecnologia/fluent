/**
   Retorna o código do grupo de parâmetros de sessão

	@author    Ricardo Gonçalves
	@date      04/08/2015
	@trigger   

   @param in_param nome do parâmetro que deverá ser criado
   @param in_value valor do parâmetro
   
   @return código o grupo de parâmetro criado
   
	Histórico
	---------------------------------------------------------------------------------------------------------------------
   Legenda: [-] - Correção / [*] - Recurso modificado/melhorado / [+] - Implementação de recurso

*/
create or replace function sys_00018####???()
returns integer as
$$
Declare
   igroup_     [(ss116)].group_%type;
Begin   
   select coalesce(max(group_), 0) + 1
     into igroup_
     from [(ss116)]
    where session = sys_session();

   return igroup_;
End;
$$
language 'plpgsql';