create or replace function sys_042text####???
(  in in_columnname varchar(30),
   out ttext text )
as $$
begin
   select text_
     into ttext
     from [(ss042)]
    where session = sys_session()
      and columnname = lower(in_columnname);
end;
$$ language plpgsql;
