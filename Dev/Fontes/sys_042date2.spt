create or replace function sys_042date2####???
(  in pTable      varchar(30),
   in pColumnName varchar(30),
   out dDate date )
as $$
begin
   select date_
     into dDate
     from [(ss042)]
    where session = sys_session()
      and CodTable   = upper(pTable)
      and columnname = lower(pColumnName);
end;
$$ language plpgsql;
