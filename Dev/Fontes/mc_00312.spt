/**
   Obtem preço do serviço da tabela de preços na data informada (ignora preço por cliente)

	@author    Ricardo Gonçalves
	@date      18/03/2011 20:48:39
	@trigger

	@param  in_tabela   Tabela de preços base
	@param  in_serv Código do serviço
	@param  in_data  Data para a qual se deseja obter o valor

	Histórico
	---------------------------------------------------------------------------------------------------------------------
   Legenda: [-] - Correção / [*] - Recurso modificado/melhorado / [+] - Implementação de recurso
*/
CREATE OR REPLACE FUNCTION mc_00312####???(
   in in_tabela integer,
   in in_serv integer,
   in in_data date)
RETURNS numeric(15, 2)
AS
$$
BEGIN
   return mc_00310####???(in_tabela, null, in_serv, in_data);
END;
$$
  LANGUAGE 'plpgsql' VOLATILE;