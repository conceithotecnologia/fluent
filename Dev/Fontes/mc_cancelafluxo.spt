Create or Replace Function mc_cancelafluxo####???
(  Out iRetorno  Integer )
As $$
Declare

-- {Variрveis de uso interno}

Begin
   iRetorno := 1;
   Update [(fc0)]
      Set fc0_estorno = fc0_saldo
    Where fc0_saldo   > 0.00
      And fc0_dtfinal < sys_getdatabase####???();
   iRetorno := 1;
End;
$$ language plpgsql;
