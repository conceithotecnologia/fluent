Create or Replace Function sys_maxval
(  In  in_nValor1 Numeric,
   In  in_nValor2 Numeric,
   Out out_nMaior Numeric )
AS $$
Begin
   out_nMaior := Case When in_nValor1 > in_nValor2 Then in_nValor1 Else in_nValor2 End;
End;
$$ LANGUAGE plpgsql;
