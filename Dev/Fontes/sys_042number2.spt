create or replace function sys_042number2####???
(  in pCodTable   varchar(30),
   in pColumnName varchar(30),
   out nNumber numeric(22, 10))
AS $$
begin
   select number_
     into nNumber
     from [(ss042)]
    where session = sys_session()
      and CodTable   = upper(pCodTable)
      and columnname = lower(pColumnName);
end;
$$ language plpgsql;
