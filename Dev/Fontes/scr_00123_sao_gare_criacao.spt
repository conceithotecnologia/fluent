/**
   Trigger de Tela da tabela SAO - Contas a Pagar Formulario GARE

	@author    Fabio Carvalho
	@date      19/09/2012
	@function

	Histórico
	---------------------------------------------------------------------------------------------------------------------
   Legenda: [-] - Correção / [*] - Recurso modificado/melhorado / [+] - Implementação de recurso

*/
Create or Replace Function scr_00123_sao_gare_criacao####???
(out out_res integer )
As $$

Declare
   rFilial    Record;
begin
   -- Recupera as configurações do cadastro de filial
   select a.*, b.*
     into rFilial
     from [(ss081)] a
     join [(ss063)] b on b.filial = a.filial;

   -- Grava Razao social da filial
   perform sys_042text_wr####???('SAO', 'sao_razao',       rFilial.razao);

   -- Tipo de Cnpj ou Cpf
   if rFilial.cnpj is not null then
      perform sys_042string_wr####???('SAO', 'g005_cod', '2');  -- Cnpj
   else
      perform sys_042string_wr####???('SAO', 'g005_cod', '1');  -- Cpf
   end if;

   -- Numero do Cnpj/Cpf
   if rFilial.cnpj is not null then
      perform sys_042string_wr####???('SAO', 'sao_identificador', rFilial.cnpj);
   else
      perform sys_042string_wr####???('SAO', 'sao_identificador', rFilial.cpf);
   end if;

   --Recupera Codigo do Fornecedor Padrao
   if rFilial.ss081_gare_forn_padrao is not null then
      perform sys_042integer_wr####???('SAO', 'ac_codforn',rFilial.ss081_gare_forn_padrao);

      -- preenche as informações basicas
      perform scr_00124_sao_gare####???();
   end if;

   out_res := 1;
End;
$$ language plpgsql;