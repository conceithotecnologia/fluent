/**
   Caso o material não possua controle de rastreabilidade exibe mensagem de erro

	@author    Ricardo Gonçalves
	@date      08/03/2004
	@trigger

   Histórico
	---------------------------------------------------------------------------------------------------------------------
   Legenda: [-] - Correção / [*] - Recurso modificado/melhorado / [+] - Implementação de recurso
*/
Create or Replace Function mc_00042####???
(  in in_codpro varchar,
   out out_result Integer ) As 
$$
Begin
   out_result := 1;

   If ck_00017####???(in_codpro, current_date) = 0 Then
      raise '[[Produto % não possui controle de rastreabilidade]]', in_codpro;   
   End If;
End;
$$ language plpgsql;
