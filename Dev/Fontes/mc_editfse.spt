create or replace function mc_editfse####???
( out out_res Integer)
As $$
begin

   -- Atribuo os novo valores
   update [(ss042)]
      set number_ = sys_042number####???('fse_valor')    -
                    sys_042number####???('fse_desconto') +
                    sys_042number####???('fse_multa')    +
                    sys_042number####???('fse_juros')   
    where session = sys_session()
      and Columnname = 'fse_valor_pago';
   out_res:= 1;
end;
$$ language plpgsql;