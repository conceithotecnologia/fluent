Create or Replace Function sys_compete(TIMESTAMP) RETURNS varchar(6) AS '
Declare

-- {Parametros de entrada}
   in_dData         Alias For $1;

Begin
    Return sys_strzero(sys_month(in_dData), 2) || sys_strzero(sys_year(in_dData), 4);
End;
' LANGUAGE 'plpgsql';



