Create or Replace Function mc_01180####???()
Returns integer As
$$
Declare
   cSession [(ss027)].session%type;
Begin
   cSession := sys_session();

   update [(sdi)] a
      set sdi_status = 2
     from [(ss029)] s 
    where s.session = cSession
      and s.codtable = 'SDI' 
      and s.recfile = a.recno
      and a.sdi_status = 1;
   
   delete 
     from [(ss029)]
    where session = cSession
      and codtable = 'SDI';

   return 1;
End;
$$
language 'plpgsql'