/**
   Retorna o conteudo do arquivo de sessão de tela de campos data

	@author 	Fabio Carvalho
	@date   14/06/2007

	@param
	@return
 */
create or replace function sys_042date####???
(   in pColumnName varchar(30),
   out dDate date )
as $$
begin
   select date_
     into dDate
     from [(ss042)]
    where session = sys_session()
      and columnname = lower(pColumnName);
end;
$$ language plpgsql;
