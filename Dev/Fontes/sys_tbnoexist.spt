create or replace function sys_tbnoexist(varchar(02)) returns setof varchar(10)
as '
select codtable
  from ss009
 where style = 1
   and (codtable || $1) not in ( select upper(tablename)
                           from pg_tables
     					  where tablename not like ''pg_%'')
union
select codtable
  from ss009
 where style = 2
   and codtable not in ( select upper(tablename)
                           from pg_tables
     					  where tablename not like ''pg_%'')
' language 'SQL';