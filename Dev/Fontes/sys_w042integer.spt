create or replace function sys_w042integer####???
(  in pCodTable   varchar(30),
   in pColumnName varchar(30),
   in pConteudo   integer,
   out iInteger integer )
as $$
begin
   Update [(ss042)] 
      Set integer_   = pConteudo
    Where session    = cSessao
      And CodTable   = pCodTable
      And Columnname = pColumnName;
end;
$$ language plpgsql;
