/**
   Obtem preço do serviço da tabela de preços na data atual (ignora preço por cliente)

	@author    Ricardo Gonçalves
	@date      18/03/2011 20:55:40
	@trigger

	@param  in_tabela   Tabela de preços base
	@param  in_serv Código do serviço

	Histórico
	---------------------------------------------------------------------------------------------------------------------
   Legenda: [-] - Correção / [*] - Recurso modificado/melhorado / [+] - Implementação de recurso
*/
CREATE OR REPLACE FUNCTION mc_00313####???(
   in in_tabela integer,
   in in_serv integer)
RETURNS numeric(15, 2)
AS
$$
BEGIN
   return mc_00310####???(in_tabela, null, in_serv, current_date);
END;
$$
  LANGUAGE 'plpgsql' VOLATILE;