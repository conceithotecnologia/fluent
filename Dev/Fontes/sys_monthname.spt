Create or Replace Function sys_monthname
(  in in_month integer,
   out outres varchar)
AS $$
Begin
  outres:= '';
  if in_month = 1 then
     outres:= 'Janeiro';
  elsif in_month = 2 then
     outres:= 'Fevereiro';
  elsif in_month = 3 then
     outres:= 'Marco';
  elsif in_month = 4 then
     outres:= 'Abril';
  elsif in_month = 5 then
     outres:= 'Maio';
  elsif in_month = 6 then
     outres:= 'Junho';
  elsif in_month = 7 then
     outres:= 'Julho';
  elsif in_month = 8 then
     outres:= 'Agosto';
  elsif in_month = 9 then
     outres:= 'Setembro';
  elsif in_month = 10 then
     outres:= 'Outubro';
  elsif in_month = 11 then
     outres:= 'Novembro';
  elsif in_month = 12 then
     outres:= 'Dezembro';
  end if;
End;
$$ LANGUAGE plpgsql;