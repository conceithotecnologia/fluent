==============================================================================*/
Create or Replace Function mc_00150####???
( Out out_res Integer )
As $$
Declare
   iCurr   [(sat)].at_curr%type;            -- Número Atual da NF      
   cSerie  [(sat)].at_serie%type;
   cSessao Varchar(35);
                                         
   -- variaveis de uso interno
   nQuantos    Integer;
Begin
   out_res := 0;
   cSessao := sys_session();
   
   -- Recupera os valores correntes na seção
   Select sai.at_serie
     Into cSerie
     from [(sai)] sai 
          Join [(ss029)] ss029 on sai.recno = ss029.recfile;
      
   if sys_042integer2####???('PARAMS','param_1') = 1 then
      -- Atribuo o novo valor no 2. Parametro
      update [(ss042)]
         set string_    = cSerie,
             enabled    = 0
       where session    = cSessao
         and Columnname = 'param_2';
   else
        update [(ss042)]
         set string_    = cSerie,
             enabled    = 0
       where session    = cSessao
         and Columnname = 'param_2';
         
      update [(ss042)]
         Set enabled    = 1
--             integer_   = coalesce((select at_curr from [(sat)] where at_serie = cSerie),1)             
       Where session    = cSessao
         and ColumnName = 'param_3';
   end if;
             
   out_res := 1;
end;
$$ language 'plpgsql'
