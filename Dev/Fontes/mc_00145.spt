Create or Replace Function mc_00145####???() 
  Returns trigger AS $$
Declare

begin
   If tg_op = 'DELETE' then
      if (mc_getflag####???('SAF',old.saf_codped) = 0) and
         (old.ga1_altera = 0) then
         raise '[[ATENÇÃO. Não é possivel excluir este registro.]]';
      end if;
      Return Old;
   End If;
   Return New;
End;
$$  LANGUAGE plpgsql;