/**
   Obtem preço do serviço da tabela de preços para um determinado cliente na data atual

	@author    Ricardo Gonçalves
	@date      18/03/2011 20:48:39
	@trigger

	@param  in_tabela   Tabela de preços base
	@param  in_cliente  Código do cliente
	@param  in_serv Código do serviço

	Histórico
	---------------------------------------------------------------------------------------------------------------------
   Legenda: [-] - Correção / [*] - Recurso modificado/melhorado / [+] - Implementação de recurso
*/
CREATE OR REPLACE FUNCTION mc_00311####???(
   in in_tabela integer,
   in in_cliente integer,
   in in_serv integer)
RETURNS numeric(15, 2)
AS
$$
BEGIN
   return mc_00310####???(in_tabela, in_cliente, in_serv, current_date);
END;
$$
  LANGUAGE 'plpgsql' VOLATILE;