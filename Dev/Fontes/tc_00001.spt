create or replace function tc_00001####???(
      in in_files integer)
returns integer as
$$
declare
   i        integer;
   fileName varchar;
begin
   for i in 1..in_files loop
      filename := 'Spool\FileTest' || to_char(i, 'FM0000') || '_' || to_char(trunc(random() * 10000), 'FM000000') || '.txt';

      insert into [(ss135)] (descri, nome, conteudo, estado)
            values           ('Arquivo teste', filename, 'Conteúdo do arquivo txt gerado via rotina tc_00001.spt do Fluent ERP. Conceitho Tecnologia.', 1);
   end loop;
        
   return 1;
end;
$$
language plpgsql;