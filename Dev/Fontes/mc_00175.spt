/**
   Estoque - Atualização dos saldos dos endereços

	@author    Gabriel Montes
	@date      24/08/2016 13:44:00
	@trigger   

	Histórico
	---------------------------------------------------------------------------------------------------------------------
   Legenda: [-] - Correção / [*] - Recurso modificado/melhorado / [+] - Implementação de recurso
*/
Create or Replace Function mc_00175####???(
   in in_data date,   
   in in_codpro varchar,
   in in_lote varchar,
   in in_ender varchar)
returns integer As 
$$
Declare
   xcur  refcursor;
   r     record;
Begin
   -- Gera parâmetro de sessão. Não criticar estoque no reprocessamento;
   perform sys_00012####???('est_criticar', 'false');
   
   -- Reprocessamento de movimentos por lote e endereço com atualização do saldo atual
   perform mc_01162####???(in_codpro, in_lote, in_ender, in_data, true);
   
   -- Valor padrão
   perform sys_00012####???('est_criticar', 'true');
   
   return 1;
End;
$$ language plpgsql;