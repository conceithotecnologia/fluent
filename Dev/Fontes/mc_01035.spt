/*============================================================================
  Empresa..: MultCont Informática
  Descrição: Procedure para validar Tabela fcc         
  Autor....: Fabio Carvalho
  Data.....: 25/08/2007 
  ---------------|------------------------------------------------------------
  out_res        | 0 - Falha / 1 - Sucesso
  ============================================================================*/
Create or Replace Function mc_01035####???( out out_res  Integer )
As $$
Declare
   rRec      Record;
   iSistema  [(ss027)].codsis%type;
Begin
   out_res := 0;

   --verifica o modulo
   select codsis
     into iSistema
     from [(ss027)]
    where session = sys_session();

   --verifica se Há registro
   select *
     into rRec
     from [(tl0)], [(fcc)];

   if not found then
      --raise '[[Arquivo de configuração não preenchido.]]';

      --verifica sé é sistema TupiGas
   elsif iSistema = 18 and
         (rRec.tl0_ak_cc        is null or
          rRec.tl0_ak_cc_vales  is null or
          rRec.tl0_ak_cc_cd     is null or
          rRec.tl0_ak_cc_cc     is null or
          rRec.tl0_ak_cc_conv   is null or
          rRec.tl0_ak_cc_cheque is null or
          rRec.f4_tes_vendas    is null) then
          --raise '[[ATENÇÃO. Não foi definido parâmetros obrigatórios no arquivo de configuração. Verifique!]]';
   end if;

   Out_res := 1;
end;
$$ language 'plpgsql'
