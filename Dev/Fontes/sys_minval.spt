Create or Replace Function sys_minval
(  In  in_nValor1 Numeric,
   In  in_nValor2 Numeric,
   Out out_nMenor Numeric )
AS $$
Begin
   out_nMenor := Case When in_nValor1 < in_nValor2 Then in_nValor1 Else in_nValor2 End;
End;
$$ LANGUAGE plpgsql;
