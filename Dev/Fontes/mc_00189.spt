Create or Replace Function mc_00189####???()
Returns trigger
As
$$
Begin
   if tg_op = 'INSERT' then
      new.lox_data := localtimestamp;
   end if;

   if tg_op = 'DELETE' then
      return old;
   else
      return new;
   end if;
End;
$$ language plpgsql;