/**
  Validação da Inscrição Estadual

   Autor	      Fabio Crepaldi Carvalho
   Data        14/11/2009 10:00

   Parâmetro   ie, pessoa             

	Histórico
	------------------------------------------------------------------
*/
Create or Replace Function sys_ie   
(  in sie   varchar,
   in sUf   varchar )
Returns void 
as $$
Declare
   ErrorCode integer;
Begin
   ErrorCode := sys_ie_check(sie, sUf);

   if    ErrorCode = 1 then
      raise '[[Tamanho indevido na Inscrição Estadual % do estado %!]]', sie, suf;
   elsif ErrorCode = 2 then
      raise '[[Inscrição Estadual % do estado % inválida!]]', sie, suf;
   end if;

End;
$$ language 'plpgsql';