/**
   Estoque - Reprocessamento do Saldo Atual

	@author    Ricardo Gonçalves
	@date      20/04/2018
	@trigger   
   
   @param in_codpro código do material que será reprocessado
   
	Histórico
	---------------------------------------------------------------------------------------------------------------------
   Legenda: [-] - Correção / [*] - Recurso modificado/melhorado / [+] - Implementação de recurso         
*/
Create or Replace Function mc_01137####???(
   in in_codpro varchar)
Returns integer As
$$
Declare 
Begin    
   if in_codpro is not null then
      perform mc_01133####???(in_codpro);
   else   
      perform mc_01133####???(b1_codpro)
         from [(e11)]
        group by b1_codpro;
   end if;

   return 1;
End;
$$
language plpgsql;