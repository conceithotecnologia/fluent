CREATE OR REPLACE FUNCTION ss_00068####???()
  RETURNS trigger AS
$$
BEGIN
   refresh materialized view ss125;
   
   if tg_op <> 'DELETE' then      
      return new;
   else
      return old;
   end if;
END;
$$
  LANGUAGE 'plpgsql' VOLATILE;