#10;     Memo28.Text := hora(&#60;DID.&#34;did_hr_ext&#34;&#62;);&#13;&#10;&#13;&#10;  nHoras     := nHoras     + &#60;DID.&#34;did_hr_dia_c&#34;&#62;;&#13;&#10;  nExtras    := nExtras    + &#60;DID.&#34;did_hr_ext_c&#34;&#62;;&#13;&#10;  nAdicional := nAdicional + &#60;DID.&#34;did_hr_adi_c&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;function Dia(pData:String):String;&#13;&#10;var i:integer;&#13;&#10;begin&#13;&#10;  i := DayOfWeek(StrToDate(pData));&#13;&#10;&#13;&#10;  if i = 1 then&#13;&#10;     Result := 'DOM'&#13;&#10;  else if i = 2 then&#13;&#10;     Result := 'SEG'&#13;&#10;  else if i = 3 then&#13;&#10;     Result := 'TER'&#13;&#10;  else if i = 4 then&#13;&#10;     Result := 'QUA'&#13;&#10;  else if i = 5 then&#13;&#10;     Result := 'QUI'&#13;&#10;  else if i = 6 then&#13;&#10;     Result := 'SEX'&#13;&#10;  else&#13;&#10;     Result := 'SAB';&#13;&#10;end;&#13;&#10;&#13;&#10;function hora(pHora:String):String;&#13;&#10;var sRetorno:string;&#13;&#10;begin&#13;&#10;  if StrToInt(copy(pHora,1,2)) &#62; 23 then&#13;&#10;     begin&#13;&#10;        sRetorno := inttostr(StrToInt(copy(pHora,1,2)) - 24);&#13;&#10;        if length(sRetorno) = 1 then&#13;&#10;           sRetorno := '0' + sRetorno;&#13;&#10;        Result :=  sRetorno + ':' + copy(pHora,3,2);&#13;&#10;     end&#13;&#10;  else if length(pHora) &#62; 0 then&#13;&#10;     Result := copy(pHora,1,2) + ':' + copy(pHora,3,2)&#13;&#10;  else&#13;&#10;     Result := '';&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  nHoras  := 0;&#13;&#10;  nExtras := 0;&#13;&#10;  nAdicional := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupFooter1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Memo31.Text := strzero(FloatToStr(int(nAdicional)),3) + ':' +&#13;&#10;                 strzero(FloatToStr(round((nAdicional - int(nAdicional)) * 60)),2);&#13;&#10;  Memo27.Text := strzero(FloatToStr(int(nHoras)),3) + ':' +&#13;&#10;                 strzero(FloatToStr(round((nHoras - int(nHoras)) * 60)),2);&#13;&#10;  Memo30.Text := strzero(FloatToStr(int(nExtras)),3) + ':' +&#13;&#10;                 strzero(FloatToStr(round((nExtras - int(nExtras)) * 60)),2);&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;function StrZero(Valor:string; QtdZero:integer):String;&#13;&#10;var&#13;&#10;  I,Tamanho:integer;&#13;&#10;  aux: string;&#13;&#10;begin&#13;&#10;  aux:=Valor;&#13;&#10;  Tamanho:=length(Valor);&#13;&#10;  Valor:='';&#13;&#10;  for I:=1 to QtdZero-tamanho do&#13;&#10;  Valor:=Valor + '0';&#13;&#10;  aux:=Valor+aux;&#13;&#10;  RESULT := aux;&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure MasterData1OnAfterPrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Memo12.Text := '';&#13;&#10;  Memo13.Text := '';&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." OnReportPrint="frxReportOnReportPrint">
