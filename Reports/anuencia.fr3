<TfrxReport Version="4.10.3" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Padrão" PrintOptions.PrintOnSheet="0" PrintOptions.ShowDialog="False" ReportOptions.CreateDate="41190,3431862731" ReportOptions.Description.Text="" ReportOptions.LastChange="41246,6704377546" ScriptLanguage="PascalScript" ScriptText.Text="var ctexto :string;                                             &#13;&#10;procedure SANan_codtitOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   SANan_codtit.Text := 'T000' + inttostr(&#60;SAN.&#34;an_codtit&#34;&#62;) + ' - P0' + inttostr(&#60;SAN.&#34;an_parce&#34;&#62;);                                                                                          &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;   // Endereço (Cabecalho)                                                                                             &#13;&#10;   Memo1.Lines.Add(&#60;SS063.&#34;ender&#34;&#62; + ',' +&#60;SS063.&#34;num&#34;&#62;);&#13;&#10;   Memo1.Lines.Add('Cep: ' + formatmasktext('99999-999;0',&#60;SS063.&#34;cep&#34;&#62;) +&#13;&#10;                   ' - ' + &#60;SS063.&#34;bairro&#34;&#62;);&#13;&#10;   Memo1.Lines.Add(&#60;SS063.&#34;cidade&#34;&#62; + '-' + &#60;SS063.&#34;uf&#34;&#62;);                                                                                   &#13;&#10;&#13;&#10;   // texto Inicial                                              &#13;&#10;   cTexto := 'Pela presente, a empresa ' + uppercase(&#60;SS063.&#34;razao&#34;&#62;) + &#13;&#10;             ', sediada à ' + &#60;SS063.&#34;ender&#34;&#62; + ',' + &#60;SS063.&#34;num&#34;&#62; +&#13;&#10;             trim(' ' +  &#60;SS063.&#34;compl&#34;&#62;) + &#13;&#10;             ', CEP: ' +&#13;&#10;             formatmaskText('99999-999;0',&#60;SS063.&#34;cep&#34;&#62;) + &#13;&#10;             ', na cidade de ' + &#60;SS063.&#34;cidade&#34;&#62; + '-' + &#60;SS063.&#34;uf&#34;&#62; + &#13;&#10;             ', inscrita no Cnpj/Cpf nº ';&#13;&#10;   if &#60;SS063.&#34;cpf&#34;&#62; &#60;&#62; '' then&#13;&#10;      cTexto := cTexto + formatmasktext('999.999.999-99;0', &#60;SS063.&#34;cpf&#34;&#62;)                                                                                 &#13;&#10;   else&#13;&#10;      cTexto := cTexto + formatmasktext('99.999.999/9999-99;0',&#60;SS063.&#34;cnpj&#34;&#62;);&#13;&#10;   cTexto := cTexto + ' e Inscrição Estadual nº ' + &#60;SS063.&#34;ie&#34;&#62; +&#13;&#10;             ', representada neste ato pelo sócio(a) Sr(a). ' +&#13;&#10;             uppercase(&#60;SS063.&#34;socio_nome&#34;&#62;) + &#13;&#10;             ', portador do RG nº ' + &#60;SS063.&#34;socio_rg&#34;&#62; +&#13;&#10;             ' e do CPF nº ' + formatmasktext('999.999.999-99;0',&#60;SS063.&#34;socio_cpf&#34;&#62;) +&#13;&#10;             ', declara que dá plena e total quitação ao débito havido contra a empresa ' +&#13;&#10;             uppercase(&#60;SAN.&#34;cliente_razao&#34;&#62;) + &#13;&#10;             ', sediada à ' + &#60;SAN.&#34;cliente_endereco&#34;&#62; +&#13;&#10;             'inscrita no Cnpj/Cpf sob nº ' + &#60;SAN.&#34;cliente_cnpj&#34;&#62; +&#13;&#10;             ', de acordo com as seguintes informações:';&#13;&#10;   Memo4.Text := cTexto;&#13;&#10;&#13;&#10;   // Desabilita se Nao Houver Nosso Numero&#13;&#10;   if &#60;SAN.&#34;an_nnumero&#34;&#62; = '' then&#13;&#10;      begin&#13;&#10;         nNumero.Visible := False;&#13;&#10;         Memo10.Visible  := False;                                                                   &#13;&#10;      end;        &#13;&#10;           &#13;&#10;   // desabilita se nao houver numero da nota&#13;&#10;   if (&#60;SAN.&#34;san_nota&#34;&#62; = 0) or&#13;&#10;      (&#60;SAN.&#34;nota_ambiente&#34;&#62; = 0) or&#13;&#10;      (&#60;SAN.&#34;at_serie&#34;&#62; = '')  then&#13;&#10;      begin&#13;&#10;         nnota.Visible  := False;&#13;&#10;         Memo11.Visible := False;                                                                   &#13;&#10;      end;        &#13;&#10;   // Desabilita se nao houver serie da nota&#13;&#10;   if (&#60;SAN.&#34;at_serie&#34;&#62; = '') or&#13;&#10;      (&#60;SAN.&#34;nota_ambiente&#34;&#62; = 0) or&#13;&#10;      (&#60;SAN.&#34;at_serie&#34;&#62; = '')  then&#13;&#10;      begin&#13;&#10;         SANat_serie.Visible := False;&#13;&#10;         Memo12.Visible      := False;                                                                                           &#13;&#10;      end;                &#13;&#10;   // texto inferior                                    &#13;&#10;   Memo13.Text := 'Declara, ainda, que nada tem a opor ao CANCELAMENTO do protesto ' +&#13;&#10;             'do titulo acima, conforme os termos da LEI 9.492 de 10/09/1997.';&#13;&#10;   // Local / Data&#13;&#10;   Memo14.Text := &#60;SS063.&#34;cidade&#34;&#62; + ', ' + datetostr(date);&#13;&#10;   // datetostr(&#60;Date&#62;);&#13;&#10;   // Nome do Socio                                   &#13;&#10;   Memo15.Text := &#60;SS063.&#34;socio_nome&#34;&#62;;&#13;&#10;   // Cpf             &#13;&#10;   Memo3.Text :=  formatmasktext('999.999.999-99;0', &#60;SS063.&#34;socio_cpf&#34;&#62;);                                          &#13;&#10;end." PropData="08446174617365747301010C2000000020446174615365743D2253414E2220446174615365744E616D653D2253414E2200010C2400000020446174615365743D2253533036332220446174615365744E616D653D225353303633220000095661726961626C65730100055374796C650100">
